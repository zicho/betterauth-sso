{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch server",
      "request": "launch",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "console": "internalConsole"
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "Launch Firefox",
      "url": "http://127.0.0.1:5173",
      "webRoot": "${workspaceFolder}",
      "reAttach": true
    }
  ],
  "compounds": [
    {
      "name": "Both",
      "configurations": [
        "Launch server",
        "Launch Firefox"
      ]
    }
  ]
}